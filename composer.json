{
    "name": "moahmedmish/laravel-price-formatter",
    "description": "A Laravel package for formatting prices based on country code and language",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mohamed Mish",
            "email": "developper.mohamed@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "orchestra/testbench": "^6.0|^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "MoahmedMish\\PriceFormatter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MoahmedMish\\PriceFormatter\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MoahmedMish\\PriceFormatter\\PriceFormatterServiceProvider"
            ],
            "aliases": {
                "PriceFormatter": "MoahmedMish\\PriceFormatter\\Facades\\PriceFormatter"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "keywords": ["laravel", "price", "formatter", "currency", "localization"],
    "homepage": "https://github.com/moahmedmish/laravel-price-formatter",
    "support": {
        "issues": "https://github.com/moahmedmish/laravel-price-formatter/issues",
        "source": "https://github.com/moahmedmish/laravel-price-formatter"
    }
}
